{
	"manifest_version": 2,
	"name": "Junsei",
	"author": "Nekoseri",
	"version": "1.0.0",
	"description": "Removes unsupported headers from responses, restoring modern websites' functionality on older web browsers.",
	"icons": {
		"16": "icons/default16.png",
		"32": "icons/default32.png",
		"48": "icons/default48.png",
		"64": "icons/default64.png",
		"128": "icons/default128.png"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	]
}